    "@storybook/preset-create-react-app": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-webpack5": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@types/jest": "^29.5.4",
    "autoprefixer": "^10.4.16",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.7",
    "webpack": "^5.89.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": "18.16.1",
    "npm": "9.5.1"
